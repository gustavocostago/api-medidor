generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Measure{
  measure_uuid            String    @id @default(uuid())
  customer_code   String 
  measure_datetime DateTime
  measure_type MeasureType
  measure_value Int
  image_url String
  has_confirmed Boolean @default(false)
  @@map("measure")
}

enum MeasureType {
  WATER
  GAS
}